{
  "name": "nuxt-blank",
  "version": "1.0.0",
  "description": "My rad Nuxt.js project",
  "author": "Sergei Kornienko",
  "private": true,
  "scripts": {
    "imagemin": "gulp imagemin",
    "svg-sprite": "svg-sprite --symbol --symbol-dest=static/images --symbol-sprite=sprite.svg assets/images/svg/*.svg",
    "swagger": "gulp swagger",
    "prepeare": "yarn imagemin && yarn svg-sprite && yarn swagger",
    "predev": "yarn prepeare",
    "prebuild": "yarn prepeare",
    "pregenerate": "yarn prepeare",
    "dev": "HOST=0.0.0.0 node --inspect node_modules/.bin/nuxt",
    "build": "nuxt build",
    "start": "yarn build && HOST=0.0.0.0 node --inspect node_modules/.bin/nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "report": "nuxt build --analyze"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "@sentry/browser": "^4.2.3",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "current-device": "^0.7.8",
    "detect-browser": "^4.0.4",
    "gsap": "^2.0.2",
    "intersection-observer": "^0.5.1",
    "lodash-es": "^4.17.11",
    "lottie-web": "^5.4.1",
    "nuxt": "^2.2.0",
    "nuxt-property-decorator": "^1.2.0",
    "stylus-mixins": "https://github.com/qluqua/stylus-mixins.git",
    "svgxuse": "^1.2.6",
    "validator": "^10.8.0",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "babel-eslint": "^8.2.1",
    "bemto.pug": "^2.1.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-exec": "^3.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-json-editor": "^2.4.2",
    "gulp-rename": "^1.4.0",
    "gulp-yaml": "^2.0.2",
    "imagemin-mozjpeg": "^8.0.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-sprite": "^1.5.0",
    "swagger-codegen": "^0.1.1",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3"
  }
}
